<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EduPlanner.Views.Student.MiPerfil">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Mi Perfil" HorizontalOptions="Center" FontSize="Large" Margin="0,20,0,10" />
            <Label Text="{Binding UserEmail}" HorizontalOptions="Center" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>